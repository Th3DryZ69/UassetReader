<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="Uasset_Reader.Workspace.Usercontrols.Overlays.ImportObjects" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:Uasset_Reader.Workspace.Usercontrols.Overlays" Height="300" Width="600">
  <Border CornerRadius="10" Background="#090B0E" BorderBrush="#050608" BorderThickness="1">
    <UIElement.Effect>
      <DropShadowEffect ShadowDepth="0" Opacity="0.3" BlurRadius="10" />
    </UIElement.Effect>
    <Grid>
      <TextBlock Text="Object Path:" TextAlignment="Left" HorizontalAlignment="Left" Foreground="#EFEFF0" FontSize="13" FontWeight="SemiBold" Margin="12,68,0,210" Height="20" />
      <TextBox Name="ObjectBox" Background="#0B0F16" BorderBrush="#050608" Foreground="#EFEFF0" Text="" VerticalContentAlignment="Center" Height="34" Margin="10,89,10,175" />
      <Button Content="Load Object Strings" Margin="438,248,10,10" Click="ImportObject_Click" />
      <Border Name="Close" Style="{StaticResource Close}" Margin="553,5,5,263" MouseLeftButtonDown="Close_Click">
        <Image Source="/Workspace/Assets/Close.png" Height="15" Width="15" IsEnabled="False" UseLayoutRounding="True" />
      </Border>
    </Grid>
  </Border>
</UserControl>